{
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "name": "Hello World Extension",
  "version": "1.0",
  "description": "A simple Hello World Chrome extension",
  "action": {
    "default_popup": "popup.html",
	"default_title": "Hello Extension",
  "permissions": [
    "tabs",
    "activeTab"
  ]
  }
}