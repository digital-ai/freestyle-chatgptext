{
  "manifest_version": 3,
  "name": "Tag Generator",
  "background": {
    "service_worker": "background.js"
  },
  "version": "1.0",
  "description": "A system that will analyze an Agility workitem and provide suggestions for tags.",
  "action": {
    "default_popup": "popup.html",
	"default_title": "Tag Generator"
  },
  "icons": {
    "16":"helper.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ]
}